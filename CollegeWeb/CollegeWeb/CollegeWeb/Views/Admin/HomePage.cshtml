@model CollegeWeb.Models.ViewModel
@{
    ViewBag.Title = "HomePage";
}

<h2>Welcome to HomePage of Admin</h2>
<div class=" form-group row col-md-12 text-right">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("Logout","Login")"

    <span>Logout</span>
</a>
</div>



<div class="form-group row col-md-12">
    <h3> Welcome @Session["Email"].ToString() </h3>

</div>

<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewStudents","Admin")"

    <span>Manage students list</span>
</a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewTeachers","Admin")"

    <span>Manage teachers list</span>
</a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewCourses","Admin")"

    <span>Manage courses</span>
</a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewSubjects","Admin")"

    <span>Manage subjects</span>
</a>
</div>
<div class=" form-group row col=md-12">

    <a type="button" class="btn btn-primary btn" href="@Url.Action("GetFilters","Admin")"

    <span>Search</span>
</a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewUserAndRoles","Admin")"

    <span>Assign other roles to users</span>
</a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ViewSubjectAndTeacher","Admin")"

    <span>Assign other subjects to teachers</span>
</a>
</div>
<script src="@Url.Content("~/Scripts/jquery.jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
<script type="text/javascript">
    function doIt(item, baseUrl) {
        window.location.href = baseUrl + '?typeOfUser=' + item.value;
    }
</script>

