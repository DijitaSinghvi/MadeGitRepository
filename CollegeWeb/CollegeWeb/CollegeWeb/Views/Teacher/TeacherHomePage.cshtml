@model CollegeWeb.Models.ViewModel
@{
    ViewBag.Title = "StudentHomePage";
}
<h2>Welcome to HomePage of Teacher</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.UserId)
}


    <div class=" form-group row col=md-12">
        <a type="button" class="btn btn-primary btn" href="@Url.Action("ShowStudentsList","Teacher", new { id = Model.UserId })"

    <span>View Students List</span>
    </a>
    </div>
    <div class=" form-group row col=md-12">
        <a type="button" class="btn btn-primary btn" href="@Url.Action("GetFilters","Search")"

    <span>Search user's details</span>
</a>
    </div>

