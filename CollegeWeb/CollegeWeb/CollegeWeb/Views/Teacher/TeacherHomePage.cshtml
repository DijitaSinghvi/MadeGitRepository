@model CollegeWeb.Models.ViewModel
@{
    ViewBag.Title = "StudentHomePage";
}
<h2>Welcome to HomePage of Teacher</h2>




<div class="form-group row col-md-12">
    <h3> Welcome @Session["Email"].ToString() </h3>

</div>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.UserId)
}

<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("EditTeacherDetails", "Teacher", new { id = Model.UserId })"

    <span>View & Edit Your Profile</span>
    </a>
</div>
<div class=" form-group row col=md-12">
    <a type="button" class="btn btn-primary btn" href="@Url.Action("ShowStudentsList","Teacher", new { id = Model.UserId })"

    <span>View the list of students you teach along with their courses.</span>
    </a>
</div>
